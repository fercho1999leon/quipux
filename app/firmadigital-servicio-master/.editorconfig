# .editorconfig

root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{java,mf,properties,xml,yml,js,html,css,scss,json}]
charset = utf-8

[*.{java,mf,properties,xml,yml}]
indent_style = tab
indent_size = 4

[*.{js,html,css,scss,json}]
indent_style = space
indent_size = 4
